<p id="notice"><%= notice %></p>

<h1>Listing User Surveys</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_surveys.each do |user_survey| %>
      <tr>
        <td><%= link_to 'Show', user_survey %></td>
        <td><%= link_to 'Edit', edit_user_survey_path(user_survey) %></td>
        <td><%= link_to 'Destroy', user_survey, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User survey', new_user_survey_path %>
