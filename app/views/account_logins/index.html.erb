<p id="notice"><%= notice %></p>

<h1>Listing Account Logins</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @account_logins.each do |account_login| %>
      <tr>
        <td><%= link_to 'Show', account_login %></td>
        <td><%= link_to 'Edit', edit_account_login_path(account_login) %></td>
        <td><%= link_to 'Destroy', account_login, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Account login', new_account_login_path %>
