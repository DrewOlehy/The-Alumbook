<h1><%= @saved_list.list_name %></h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Program</th>
      <th>Remove</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= @login.first_name + ' ' + @login.last_name %></td>
